{
  "expo": {
    "name": "fosscat",
    "slug": "fosscat",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/Icon_1024x1024.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/gradient-with-logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "net.jcxldn.fosscat"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "net.jcxldn.fosscat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
      "expo-camera", {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera in order to scan barcodes."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos in order to add pictures to items."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
